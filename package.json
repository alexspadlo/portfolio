{
  "name": "portfolio",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "alexinstall": "yarn install && rimraf .husky && yarn husky install && yarn husky add .husky/pre-commit 'yarn lint-staged' && yarn husky add .husky/pre-push 'yarn testcover'",
    "myscripts": "yarn test -u && yarn lint:ts && yarn lint:scss && git add --all",
    "start": "cross-env NODE_ENV=development webpack server",
    "build": "yarn build:clean && cross-env NODE_ENV=production webpack",
    "build:clean": "rimraf ./build",
    "test": "jest --config jestconfig.json --silent",
    "testcover": "jest --config jestconfig.json --coverage",
    "lint": "yarn lint:ts && yarn lint:scss",
    "lint:ts": "eslint ./src/**/*.tsx",
    "lint:scss": "stylelint 'src/**/*.scss' --custom-syntax postcss-scss --fix"
  },
  "lint-staged": {
    "src/**/*.tsx": [
      "yarn lint:ts"
    ],
    "src/**/*.scss": [
      "yarn lint:scss"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.0.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.1",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.3",
    "jest-mock": "^29.0.3",
    "jest-trx-results-processor": "^3.0.1",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "4.14.1",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.1",
    "postcss-scss": "^4.0.4",
    "react-test-renderer": "^18.2.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.11.0",
    "stylelint-config-standard": "^28.0.0",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@google/model-viewer": "^3.0.0",
    "bootstrap": "4.6.0",
    "jquery": "^3.6.1",
    "lottie-web": "^5.10.2",
    "popper.js": "^1.16.1",
    "react": "^18.2.0",
